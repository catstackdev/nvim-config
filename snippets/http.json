  },
  "GraphQL Query": {
    "prefix": "graphql",
    "body": [
      "POST ${1:http://localhost:8000/graphql}",
      "Content-Type: application/json",
      "",
      "{",
      "  \"query\": \"${2:query { __typename }}\"",
      "}",
      "HTTP ${3:200}",
      "${0}"
    ],
    "description": "GraphQL query"
  },
  "File Upload": {
    "prefix": "upload",
    "body": [
      "POST ${1:http://localhost:8000/upload}",
      "Content-Type: multipart/form-data; boundary=----WebKitFormBoundary",
      "",
      "------WebKitFormBoundary",
      "Content-Disposition: form-data; name=\"file\"; filename=\"${2:file.txt}\"",
      "Content-Type: ${3:text/plain}",
      "",
      "${4:file content}",
      "------WebKitFormBoundary--",
      "HTTP ${5:200}",
      "${0}"
    ],
    "description": "File upload request"
  },
  "Query Parameters": {
    "prefix": "query",
    "body": "?${1:param}=${2:value}&${3:param2}=${4:value2}",
    "description": "Query parameters"
  },
  "CRUD - Create": {
    "prefix": "crud-create",
    "body": [
      "### Create ${1:Resource}",
      "POST ${2:http://localhost:8000}/${3:resources}",
      "Content-Type: application/json",
      "",
      "{",
      "  \"${4:name}\": \"${5:value}\"",
      "}",
      "HTTP 201",
      "${0}"
    ],
    "description": "CRUD Create operation"
  },
  "CRUD - Read All": {
    "prefix": "crud-list",
    "body": [
