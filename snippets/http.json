      "PUT ${2:http://localhost:8000}/${3:resources}/${4:{{id}}}",
      "Content-Type: application/json",
      "",
      "{",
      "  \"${5:name}\": \"${6:updated value}\"",
      "}",
      "HTTP 200",
      "${0}"
    ],
    "description": "CRUD Update operation"
  },
