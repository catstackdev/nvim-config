    ],
    "description": "CRUD Delete operation"
  },
  "Full CRUD Suite": {
    "prefix": "crud-all",
    "body": [
      "### Create ${1:Resource}",
      "POST ${2:http://localhost:8000}/${3:resources}",
      "Content-Type: application/json",
      "",
      "{",
      "  \"${4:name}\": \"${5:value}\"",
      "}",
      "HTTP 201",
      "",
      "### Get All ${1:Resource}s",
      "GET ${2:http://localhost:8000}/${3:resources}",
      "HTTP 200",
      "",
      "### Get ${1:Resource} by ID",
      "GET ${2:http://localhost:8000}/${3:resources}/{{id}}",
      "HTTP 200",
      "",
      "### Update ${1:Resource}",
      "PUT ${2:http://localhost:8000}/${3:resources}/{{id}}",
      "Content-Type: application/json",
      "",
      "{",
      "  \"${4:name}\": \"updated value\"",
      "}",
      "HTTP 200",
      "",
      "### Delete ${1:Resource}",
      "DELETE ${2:http://localhost:8000}/${3:resources}/{{id}}",
      "HTTP 204",
      "${0}"
    ],
    "description": "Complete CRUD operations suite"
